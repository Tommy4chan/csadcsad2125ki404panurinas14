{
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.jsx?$",
      "excludePattern": "(node_modules/|dist/)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "./node_modules/clean-jsdoc-theme"
    }
  }
  